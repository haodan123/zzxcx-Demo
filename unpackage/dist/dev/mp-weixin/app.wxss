.search {
  background-color: #fff;
  margin-bottom: 20rpx;
}
.bzHeader {
  position: relative;
  background-color: #fff;
  margin-bottom: 20rpx;
  padding: 10px 20px;
  display: flex;
  flex-direction: column;
  gap: 10px;
}
.bzHeader .bzItem {
  display: flex;
  gap: 10px;
  align-items: center;
}
.bzHeader .bzItem .text {
  min-width: 100px;
}
.bzHeader .bzItem .select {
  flex: 1;
}
.bzContent {
  position: relative;
  width: 90%;
  font-size: 28rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  gap: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.bzContent .contentItem {
  margin-bottom: 10px;
  display: flex;
  gap: 10px;
  flex-direction: column;
}
.bzContent .contentItem .title {
  font-size: 18px;
}
.bzContent .contentItem .neirong-img {
  display: grid;
  grid-template-columns: repeat(3, 1fr);
  gap: 10px;
}
.bzContent .contentItem .neirong-img .image {
  height: 100px;
}
.bzContent .contentItem .neirong-img .image image {
  width: 100%;
  height: 100%;
}
.viewport {
  position: relative;
  background-color: #f4f4f4;
  min-height: 100vh;
}
.gapBox {
  height: 120rpx;
}
.showCard {
  position: relative;
  width: 90%;
  font-size: 28rpx;
  margin: 0 auto;
  margin-bottom: 20rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  gap: 20rpx;
  border-radius: 20rpx;
  background-color: #fff;
}
.showCard .title {
  display: flex;
  justify-content: space-between;
  align-items: center;
  height: 80rpx;
  border-bottom: 1rpx solid #ccc;
}
.showCard .card-item {
  display: flex;
  gap: 10rpx;
}
.showCard .card-item .item-text {
  margin-left: 20rpx;
}
.showCard .card-item .item-key {
  min-width: 120rpx;
}
.showCard .card-item .item-value {
  flex: 1;
}
.showCard .card-btm {
  display: flex;
  justify-content: center;
  align-items: center;
  padding: 15rpx;
  border: 1rpx solid #ccc;
  background-color: #4999D0;
  color: #fff;
  width: 200rpx;
  margin: 0 auto;
  border-radius: 40rpx;
}
.showCard .card-tag {
  padding: 15rpx 20rpx;
  border: 1rpx solid #4999D0;
  color: #4999D0;
  position: absolute;
  right: 20rpx;
  top: 20rpx;
  border-radius: 40rpx;
}
.addButton {
  position: fixed;
  bottom: 20rpx;
  left: 50%;
  transform: translateX(-50%);
  width: 50%;
  text-align: center;
  border: 1rpx solid #ccc;
  background-color: #4999D0;
  color: #fff;
  padding: 15rpx;
  border-radius: 40rpx;
}
.addButton1 {
  position: fixed;
  bottom: 20rpx;
  left: 50%;
  z-index: 9;
  transform: translateX(-50%);
  width: 50%;
  text-align: center;
  border: 1rpx solid #ccc;
  background-color: #4999D0;
  color: #fff;
  border-radius: 40rpx;
}
.formCard {
  width: 90%;
  font-size: 26rpx;
  margin: 0 auto;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
  gap: 20rpx;
  background-color: #fff;
}
.formCard .mockItem {
  margin-bottom: 22px;
  display: flex;
  flex-direction: column;
  gap: 5px;
}
.formCard .mockItem .label {
  display: flex;
  flex-direction: row;
  align-items: center;
  text-align: left;
  font-size: 14px;
  color: #606266;
  height: 36px;
  padding: 0 12px 0 0;
  vertical-align: middle;
  flex-shrink: 0;
  box-sizing: border-box;
}
.box-y-center {
  display: flex;
  align-items: center;
  height: 100%;
}
.image-text {
  text-align: left;
  font-size: 14px;
  color: #606266;
}
.saoma {
  position: absolute;
  right: 65rpx;
  top: 50%;
  transform: translateY(-50%);
  width: 50rpx;
  height: 50rpx;
  z-index: 99;
}
.card-btn {
  color: #fff;
  border-radius: 10px;
  padding: 5px 15px;
  background-color: #4898D0;
}
